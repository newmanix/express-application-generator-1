{{!-- depending on the value of the listExists variable --}}
{{!-- if itâ€™s true then the list will be rendered --}}
{{!-- if the value is equal to false the list will not be rendered --}}
{{#if listExists}} 
    {{#each users}}
      <p>
        <b>id:</b> {{this.id}}<br />
        <b>first_name:</b> {{this.first_name}}<br />
        <b>last_name:</b> {{this.last_name}}<br />
        <b>email:</b> {{this.email}} 
      </p>
    {{/each}}
    
{{else}}
  <p>Currently there are no users</p>
{{/if}}
